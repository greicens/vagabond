
<% @user.posts.each do |post|  %>
<div class="card horizontal">
  <div class="card-stacked">
    <div class="card-content">
      <span class="card-title">
        <%= post.title %>
      </span>
      <p>
        <%= post.message %>
      </p>
    </div>
    <div class="card-action">
      <%= link_to "Edit", "#edit-post" %>
      <%= link_to "Delete", "#delete-post" %>
    </div>
  </div>
</div>
<% end %>
